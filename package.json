    "express": "~4.16.1",
    "faker": "^5.5.3",
    "http-errors": "~1.6.3",
    "mocha": "^9.0.1",
    "morgan": "~1.9.1",
    "pg": "^8.6.0",
    "pm2": "^5.1.0",
    "pug": "2.0.0-beta11",
    "twitter": "^1.7.1",
    "twitter-error-handler": "^3.1.0"
  }
